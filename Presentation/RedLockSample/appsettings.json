{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "RedisConfiguration": {

    "BaseUrl": "YourRedisServerBaseURL",
    "Port": 1111,
    "Password": "YourRedisPassword",

    "ExpiryTimeFromSeconds": 5,
    "WaitTimeFromSeconds": 3,
    "RetryTimeFromMilliseconds": 200,
    "LogLockingProcess" :  "true"
  }
}
